[user]
	name = Joe Humrickhouse
	email = jhumrickhouse@fulcrum.net

[alias]
	br = branch
	st = status
	ss = stash show
	sp = stash pop
	co = checkout
	pr = pull --rebase
	unstage = reset HEAD --
	s = show --pretty=\"%ci%n sha: %Cblue%h%Creset%n  by: %Cblue%an%Creset <%e>%nsubj: %s\"
	sr = show --pretty=raw
	hist = log --color --pretty=format:\"%C(yellow)%h%C(reset) %s%C(bold red)%d%C(reset) %C(green)%ad%C(reset) %C(blue)[%an]%C(reset)\" --relative-date --decorate
	graph = log --color --graph --pretty=format:\"%h | %ad | %an | %s%d\" --date=short

[core]
	excludesfile = /Users/joe/.gitignore_global
	autocrlf = input

[push]
	default = simple
